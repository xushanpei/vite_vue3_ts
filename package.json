    "vfonts": "^0.1.0",
    "vue": "^3.2.2",
    "vue-router": "4"
  },
  "devDependencies": {
    "@babel/types": "^7.16.0",
    "@types/node": "^16.11.10",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.4.0",
    "dart-sass": "^1.25.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "mrm": "^3.0.10",
    "postcss": "^8.4.4",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "typescript": "^4.5.2",
    "vite": "^2.4.2",
